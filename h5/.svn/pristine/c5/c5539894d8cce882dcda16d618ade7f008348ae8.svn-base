const doubleCountHandle = (num) => {
    if(Object.prototype.toString.call(num) === "[object Number]") {
      return num > 9 ? num : '0' + num
    } else {
      return ''
    }
  }
const tfphoneNumber = (num) => {
  var myreg=/^[1][3,4,5,7,8,9][0-9]{9}$/;
  if (!myreg.test(num)) {
      return false;
  } else {
      return true;
  }
}

  module.exports = { doubleCountHandle, tfphoneNumber }