html, body, div, ul, li, h1, h2, h3, h4, h5, h6, p, dl, dt, dd, ol, form, input, textarea, th, td, select {
    margin: 0;
    padding: 0;
  }
  *{box-sizing: border-box;}
  html, body {
      min-height: 100%;
  }

  body {
      font-family: "Microsoft YaHei";
      font-size:14px;
      color:#333;
  }
  html, body {  
    width: 100%;  
    height: 100%;  
    margin: 0;  
    padding: 0;  
    position: relative;  
    top: 0;
    bottom: 0;
  }  
  #app {  
    width: 100%;  
    height: 100%;  
    background: #fff;  
    position: absolute;  
    left:0;  
    top:0; 
    overflow: scroll;  
    -webkit-overflow-scrolling: touch;
  }  
  #app::-webkit-scrollbar{
    display: none;
}
  h1, h2, h3, h4, h5, h6{font-weight:normal;}
  ul,ol {
      list-style: none;
  }

  img {
      border: none;
      vertical-align: middle;
  }

  a {
      text-decoration: none;
      color: #232323;
  }

  table {
      border-collapse: collapse;
      table-layout: fixed;
  }

  input, textarea, button {
      outline: none;
      border: none;
  }

  textarea {
      resize: none;
      overflow: auto;
  }

  .clearfix {
      zoom: 1;
  }

  .clearfix:after {
      content: ".";
      width: 0;
      height: 0;
      visibility: hidden;
      display: block;
      clear: both;
      overflow:hidden;
  }

  .fl {
      float: left
  }

  .fr {
      float: right
  }

  .tl {
      text-align: left;
  }

  .tc {
      text-align: center
  }

  .tr {
      text-align: right;
  }

  .ellipse {
      overflow: hidden;
      text-overflow: ellipsis;
      white-space: nowrap;
  }
  .inline{
      display: inline-block;
      *display: inline;
      *zoom: 1;
  }
  video {
      display: block;
  }