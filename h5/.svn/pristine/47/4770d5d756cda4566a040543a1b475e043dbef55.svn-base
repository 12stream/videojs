.all {
    padding-bottom: 1rem;
    background: #EDEEF1;
}
.all video {
    object-fit:fill;
    width: 100%;
    height: 19.3rem;   
    display: block;
}
.all .video-introduct {  
    background: #fff;
}
.all .video-introduct .title {
    font-size: 1.6rem;
    color: #333333;
    font-weight: bold;
    line-height: 2.3rem;
    padding: 1rem 0 0.6rem 2rem;
}
.all .video-introduct .time {
    font-size: 1.3rem;
    color: #9b9b9b;
    line-height: 1.9rem;
    padding:0 0 1rem 2rem;
}